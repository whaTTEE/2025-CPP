#include <iostream>
#include <string>
using namespace std;

int main() {
    string str1("Hello"); //생성자 호출로 초기화
    cout << str1 << endl;

     cout << str1[0] << endl;
     cout << str1[1] << endl;
     cout << str1[2] << endl;
     cout << str1[3] << endl;
     cout << str1[4] << endl;

    return 0;
}
